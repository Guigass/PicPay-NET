    <PackageIconUrl>https://res.cloudinary.com/matmiranda/image/upload/v1548887430/PicPay_icon.png</PackageIconUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>PicPay</PackageTags>
    <Authors>PicPay, Matheus Miranda, Guilherme Gentile</Authors>
    <Description>Biblioteca para integração dos meios de pagamento do PicPay</Description>
